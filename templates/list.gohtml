<!doctype html>
<html lang=en>
<meta name=viewport content="width=device-width, initial-scale=1.0">
<style>
{{.Style}}
</style>
<title>Short URLs</title>
<h1>Short URLs</h1>
<div style="overflow-x:auto;">
<table>
<thead>
<tr><th>slug</th><th>hits</th><th>url</th></tr>
</thead>
<tbody>
{{range .Data}}<tr><td>{{.Slug}}</td><td>{{.Hits}}</td><td>{{.URL}}</td></tr>{{end}}
</tbody>
</table>
</div>
</html>